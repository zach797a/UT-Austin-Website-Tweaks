                             
                     



function searchCheck() {
var unique;          
var option_selected;
{                    
for (var i=0; i < document.regform.s_request.length; i++)
   {
   if (document.regform.s_request[i].checked)
      {
      option_selected = true;        
      if (document.regform.s_request[i].value =="STGAC"){
        document.regform.action="searchClasses.WBX";   
        return true;  
        }                                       

      if (document.regform.s_request[i].value =="STGOF"){
        document.regform.action="optionalFees.WBX";
        return true;     
        }            
     }                       
   }
                                            
  }

return true;
}


